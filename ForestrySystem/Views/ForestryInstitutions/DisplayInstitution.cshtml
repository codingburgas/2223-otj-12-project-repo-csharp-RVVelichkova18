@model ForestryInstitution

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@Model.Name</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/ForestrySystem.styles.css" asp-append-version="true" />
</head>
<body>
    <div class="container">
        <h1>@Model.Name</h1>
        
                <p><strong>Location:</strong> @Model.Location</p>
                <p><strong>Phone:</strong> @Model.Phone</p>
                <p><strong>Description:</strong> @Model.Description</p>
                <div id="map"><iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d7325.687993273408!2d27.402337617736844!3d42.45285393459321!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x40a6eb7e8a995853%3A0x9f3b508a4516b2ad!2sProfessional%20Technical%20School!5e0!3m2!1sen!2sbg!4v1680857892574!5m2!1sen!2sbg" width="600" height="450" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe></div>
            

                <h2>Данни за общата сеч по години</h2>
        <div class="form-group">
            <label for="criteria">Година:</label>
            <select class="form-control" id="criteria">
                <option value="name">2020</option>
                <option value="location">2021</option>
                <option value="type">2022</option>
            </select>
        </div>


        <h2>Данни за общата сеч по категории</h2>
        <div class="form-group">
            <label for="criteria">Година:</label>
            <select class="form-control" id="criteria">
                <option value="name">2020</option>
                <option value="location">2021</option>
                <option value="type">2022</option>
            </select>
        </div>
                <table class="table">
                    <thead>
                        <tr>
                            <th>CategoryOfTimber</th>
                            <th>LargeForConstruction</th>
                            <th>Value 2</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td>Item 1</td>
                            <td>10</td>
                            <td>20</td>
                        </tr>
                        <tr>
                            <td>Item 2</td>
                            <td>30</td>
                            <td>40</td>
                        </tr>
                        <tr>
                            <td>Item 3</td>
                            <td>50</td>
                            <td>60</td>
                        </tr>
                    </tbody>
                </table>
    </div>

      </body>
</html>